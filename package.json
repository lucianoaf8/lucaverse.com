{
  "name": "lucaverse-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx kill-port 5155 && node -e \"setTimeout(() => process.exit(0), 2000)\" && vite --port 5155 --host 0.0.0.0 --strictPort --open",
    "build": "vite build",
    "preview": "npx kill-port 5155 && node -e \"setTimeout(() => process.exit(0), 2000)\" && vite preview --port 5155 --host 0.0.0.0 --strictPort --open",
    "deploy:auth": "cd lucaverse-auth && npm run deploy",
    "deploy:forms": "cd summer-heart-worker && npm run deploy",
    "deploy:workers": "npm run deploy:auth && npm run deploy:forms"
  },
  "dependencies": {
    "dompurify": "^3.2.6",
    "i18next": "^25.3.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "particles.js": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "kill-port": "^2.0.1",
    "vite": "^7.0.6"
  }
}
